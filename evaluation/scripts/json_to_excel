import json
import pandas as pd

json_file_path = "evaluation/data/EleutherAI__gpt-neo-125M/results_2025-02-11T19-45-42.135976.json"
excel_file_path = "evaluation/output/EleutherAI__gpt-neo-125M/results_2025-02-11T19-45-42.135976.xlsx"

with open(json_file_path, "r", encoding="utf-8") as file:
    data = json.load(file)

results = data.get("results", {})

data_list = []
for key, value in results.items():
    alias = value.get("alias", key)
    acc = value.get("acc,none", None)
    acc_stderr = value.get("acc_stderr,none", None)
    
    data_list.append({
        "Category": key,
        "Alias": alias,
        "Accuracy": acc,
        "Accuracy Stderr": acc_stderr
    })

df = pd.DataFrame(data_list)


df.to_excel(excel_file_path, index=False)
print(f"Excel 파일 파싱 완료!!: {excel_file_path}")
